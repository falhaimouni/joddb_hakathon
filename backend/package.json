{
    "name": "employee-tracking-backend",
    "version": "1.0.0",
    "description": "Employee tracking system backend with Express and PostgreSQL",
    "main": "server.js",
    "scripts": {
        "start": "node server.js",
        "dev": "nodemon server.js",
        "migrate": "sequelize-cli db:migrate",
        "migrate:undo": "sequelize-cli db:migrate:undo",
        "migrate:reset": "node reset-database.js && npm run migrate",
        "seed": "sequelize-cli db:seed:all",
        "seed:undo": "sequelize-cli db:seed:undo:all",
        "seed:specific": "sequelize-cli db:seed --seed"
    },
    "keywords": [
        "express",
        "postgresql",
        "sequelize"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "bcrypt": "^5.1.1",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "jsonwebtoken": "^9.0.2",
        "pg": "^8.11.3",
        "pg-hstore": "^2.3.4",
        "sequelize": "^6.35.0"
    },
    "devDependencies": {
        "nodemon": "^3.0.1",
        "sequelize-cli": "^6.6.2"
    }
}
